<h1>계획</h1>
<div id="newPlanner">
    <form action='/t/<%= team._id %>/planner' method="POST">
        <input type="text" name="title" placeholder="새 계획"><br>
        <textarea name="discription" cols="30" rows="3" maxlength="60">설명</textarea><br>
        <input class="hidden" type="text" name="teamid" value="<%= team._id %>">
        <button type="submit">완료</button>
    </form>
</div>
<ul>
    <% for (let i = 0; i < planner.length; i++) { %>
        <li>
            <a href="/p/<%= planner[i]._id%>"><%= planner[i].title %></a>
            <div class="discription">
                <%= planner[i].discription %>
            </div>
            <div class="plannerAuthor">
                writer: <%= planner[i].author %>
            </div>
        </li>
    <% } %>
</ul>